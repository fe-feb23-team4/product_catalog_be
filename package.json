{
  "name": "product-catalog-back-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "prettier": "npx prettier --write . --ignore-path .gitignore",
    "lint": "eslint . --ignore-path .gitignore",
    "fix-style": "npm run prettier && npm run lint -- --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "npm run lint",
    "*.ts": "npm run lint"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@mate-academy/eslint-config": "0.0.25",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "typescript": "^5.1.3"
  }
}
