{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "@mate-academy/eslint-config"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "ignorePatterns": ["/build"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "linebreak-style": 0
  }
}
